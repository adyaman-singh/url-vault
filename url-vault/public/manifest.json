{
  "manifest_version": 3,
  "name": "URL Vault",
  "version": "1.0.0",
  "description": "Store and manage your favorite URLs in a beautiful, minimal interface.",
  "action": {
    "default_popup": "src/popup/popup.html"
  },
  "permissions": ["storage", "tabs"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "128": "url-vault.png"
  }
}
